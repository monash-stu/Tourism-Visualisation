{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "autosize": {"type": "fit", "contains": "padding"},
    "data": {
        "url": "https://raw.githubusercontent.com/monash-stu/Tourism-Visualisation/main/data/total-visitors-trend.csv",
        "format": {"type": "csv"}
    },
    "mark": "line",
    "encoding": {
        "x": {
            "field": "Date",
            "type": "temporal",
            "title": "Date"
        },
        "y": {
            "field": "Visitors",
            "type": "quantitative",
            "title": "Number of Visitors"
        },
        "tooltip": [
            {"field": "Date", "type": "temporal", "title": "Date"},
            {"field": "Visitors", "type": "quantitative", "title": "Number of Visitors"}
        ]
    }
}  